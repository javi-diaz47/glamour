{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "import/prefer-default-export": "off",
        "global-require": "off",
        "react/jsx-props-no-spreading": "off",
        "eslint-disable-next-line": "off",
        "jsx-a11y/anchor-is-valid": "off" 
    }
}
